{
  "name": "webapp",
  "version": "1.0.2",
  "main": "electron.js",
  "scripts": {
    "ng": "ng",
    "config-dev": "ts-node set-env.mjs",
    "config-prod": "ts-node set-env.mjs -- prod",
    "start": "export API_URL='http://localhost:3000' && npm run config-dev && ng serve --disable-host-check",
    "start-gitpod": "export API_URL=$(gp url 3000) && npm run config-dev && ng serve --disable-host-check",
    "start-electron": "electron .",
    "build": "npm run config-prod && ng build --configuration production",
    "build-local": "export API_URL='http://localhost:3002' && npm run config-dev && ng build",
    "build-electron": "npm run config-prod && ng build --configuration production --base-href ./",
    "bundle-electron": "./node_modules/.bin/electron-builder",
    "test": "ng test"
  },
  "private": true,
  "dependencies": {
    "@angular/animations": "^14.3.0",
    "@angular/common": "^14.3.0",
    "@angular/compiler": "^14.3.0",
    "@angular/core": "^14.3.0",
    "@angular/forms": "^14.3.0",
    "@angular/material": "^14.2.7",
    "@angular/platform-browser": "^14.3.0",
    "@angular/platform-browser-dynamic": "^14.3.0",
    "@angular/router": "^14.3.0",
    "@anthropic-ai/sdk": "^0.19.1",
    "ace-builds": "^1.4.14",
    "aws-amplify": "^3.4.3",
    "aws-amplify-angular": "^6.0.26",
    "bootstrap": "^5.1.3",
    "clipboard": "^2.0.11",
    "dotenv": "^5.0.0",
    "electron-log": "^4.4.7",
    "electron-updater": "^5.0.1",
    "font-awesome": "^4.7.0",
    "jwt-decode": "^3.1.2",
    "marked": "^4.3.0",
    "ngx-markdown": "^14.0.1",
    "openai": "^4.3.0",
    "prismjs": "^1.29.0",
    "rxjs": "~7.5.0",
    "tslib": "^2.3.0",
    "zone.js": "~0.11.4"
  },
  "devDependencies": {
    "@angular-devkit/build-angular": "^14.2.13",
    "@angular/cli": "^14.2.13",
    "@angular/compiler-cli": "^14.3.0",
    "@types/jasmine": "~3.10.0",
    "@types/marked": "^4.0.8",
    "@types/node": "^12.11.1",
    "electron": "^24.1.3",
    "electron-builder": "^24.3.0",
    "electron-icon-maker": "^0.0.5",
    "electron-installer-dmg": "^4.0.0",
    "electron-notarize": "^1.2.1",
    "electron-packager": "^17.1.2",
    "jasmine-core": "~4.0.0",
    "karma": "~6.3.0",
    "karma-chrome-launcher": "~3.1.0",
    "karma-coverage": "~2.1.0",
    "karma-jasmine": "~4.0.0",
    "karma-jasmine-html-reporter": "~1.7.0",
    "ts-node": "~5.0.1",
    "typescript": "~4.8.4"
  },
  "optionalDependencies": {
    "appdmg": "^0.6.6"
  }
}
